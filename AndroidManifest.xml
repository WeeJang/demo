            </intent-filter>
        </activity>
-->

		<receiver 
	    	android:name="commlab.weejang.demo.BootBroadcastReceiver">
	    	<intent-filter >
	        	<action android:name="android.intent.action.BOOT_COMPLETED" />
	    	</intent-filter>	    
		</receiver>

	     <service
            android:enabled="true"
            android:name=".MeasureService"
            android:exported="true"
